{
  "name": "@commercelayer/cli",
  "version": "0.0.0",
  "description": "Commerce Layer CLI",
  "author": "Alessandro Casazza",
  "repository": "",
  "license": "MIT",
  "main": "./lib/index.js",
  "bin": {
    "commercelayer": "bin/commercelayer"
  },
  "keywords": [
    "cli",
    "commercelayer",
    "typescript",
    "javascript",
    "nodejs"
  ],
  "scripts": {
    "start": "npm run build && npm run global",
    "test": "npm run lint && npm run test:unit",
    "test:unit": "mocha src/**/*.spec.ts",
    "lint": "tslint -p . --fix",
    "build": "npm run clean:some && tsc -p .",
    "global": "npm i -g",
    "clean:some": "rm -rf ./lib ./docs",
    "clean:all": "rm -rf ./node_modules ./package-lock.json ./lib ./docs",
    "refresh": "npm run clean:all && npm install",
    "tscov": "tscov --min-coverage 90",
    "tscov:d": "tscov --details",
    "publish-package": "npm run build && npm publish",
    "docs": "typedoc --out docs ./src"
  },
  "dependencies": {
    "@commercelayer/js-auth": "^2.0.2",
    "clear": "^0.1.0",
    "commander": "^6.2.0",
    "figlet": "^1.5.0",
    "fs-extra": "^9.0.1",
    "inquirer": "^7.3.3",
    "inversify": "^5.0.5",
    "kleur": "^4.1.3",
    "lodash": "^4.17.20",
    "node-emoji": "^1.10.0",
    "reflect-metadata": "^0.1.13",
    "tslib": "^2.0.3",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@liftr/tscov": "^2.0.0",
    "@types/clear": "^0.1.1",
    "@types/figlet": "^1.2.1",
    "@types/fs-extra": "^9.0.4",
    "@types/inquirer": "^7.3.1",
    "@types/lodash": "^4.14.165",
    "@types/node": "^14.14.12",
    "@types/node-emoji": "^1.8.1",
    "@types/validator": "^13.1.1",
    "ts-node": "^9.1.1",
    "typedoc": "^0.19.2",
    "typescript": "^4.1.2"
  }
}
