{
  "name": "@commercelayer/cli",
  "description": "Commerce Layer CLI",
  "version": "5.4.1",
  "author": "Pierluigi Viti <pierluigi@commercelayer.io>",
  "bin": {
    "commercelayer": "./bin/run",
    "cl": "./bin/run",
    "clayer": "./bin/run"
  },
  "bugs": "https://github.com/commercelayer/commercelayer-cli/issues",
  "devDependencies": {
    "@commercelayer/cli-dev": "^0.1.6",
    "@oclif/test": "^2.1.0",
    "@types/chai": "^4.3.0",
    "@types/configstore": "^4.0.0",
    "@types/inquirer": "^8.1.3",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.19",
    "@types/update-notifier": "^5.1.0",
    "chai": "^4.3.6",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.2.0",
    "mocha": "^9.2.2",
    "nyc": "^15.1.0",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "engines": {
    "node": ">=16"
  },
  "files": [
    "/bin/run.*",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/commercelayer/commercelayer-cli",
  "keywords": [
    "oclif",
    "ecommerce",
    "cli",
    "commercelayer"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "commercelayer",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-plugins",
      "@oclif/plugin-autocomplete"
    ],
    "aliases": {
      "blocked": null
    },
    "hooks": {
      "init": [
        "./lib/hooks/init/update"
      ],
      "prerun": [
        "./lib/hooks/prerun/login",
        "./lib/hooks/prerun/plugin"
      ],
      "postrun": [
        "./lib/hooks/postrun/save",
        "./lib/hooks/postrun/autocomplete"
      ]
    },
    "topics": {
      "applications": {
        "description": "Manage login to CLI applications"
      },
      "plugins": {
        "description": "Manage CLI plugins"
      },
      "config": {
        "description": "Manage CLI configuration",
        "hidden": true
      },
      "cli": {
        "description": "Core CLI commands",
        "hidden": true
      }
    },
    "helpClass": "./lib/help",
    "repositoryPrefix": "<%- repo %>/blob/main/<%- commandPath %>",
    "additionalHelpFlags": [
      "-h"
    ],
    "topicSeparator": ":"
  },
  "repository": "https://github.com/commercelayer/commercelayer-cli",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif manifest && npm run readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "readme": "cl-cli-dev readme --multi && git add README.md && git add docs/*"
  },
  "types": "lib/index.d.ts",
  "dependencies": {
    "@commercelayer/cli-core": "^1.3.15",
    "@commercelayer/js-auth": "^2.3.0",
    "@commercelayer/sdk": "4.11.0",
    "@oclif/core": "^1.6.3",
    "@oclif/plugin-autocomplete": "^1.2.0",
    "@oclif/plugin-help": "5.1.12",
    "@oclif/plugin-not-found": "^2.3.1",
    "@oclif/plugin-plugins": "^2.1.0",
    "configstore": "^5.0.1",
    "inquirer": "^8.2.0",
    "lodash": "^4.17.21",
    "tslib": "^2.3.1",
    "update-notifier": "^5.1.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
