#!/usr/bin/env node
/* eslint-disable node/no-unpublished-bin */

const oclif = /* require('@oclif/core') */ require('../lib/patches/oclif/core/main')

oclif.run()
	.then(require('@oclif/core/flush'))
	.catch(error => {
		const oclifHandler = require('@oclif/core/handle')
		if (!error.message === 'HOOK_EXIT') return oclifHandler(error)
	})
