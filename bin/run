#!/usr/bin/env node

const PATCH = false

const oclif = PATCH? require('../lib/patches/oclif/core/main') : require('@oclif/core')

oclif.run()
	.then(require('@oclif/core/flush')(60000))
	.catch(error => {
		const oclifHandler = require('@oclif/core/handle')
		if (error.message !== 'HOOK_EXIT') return oclifHandler(error)
	})
